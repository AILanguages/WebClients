@using TSTuring2015.MVCWeb.Models
@model List<TSTuring2015.ScreenModels.PartDisplays.AbstractPartDisplay>

@for (int i = 0; i < @Model.Count; i++)
{
    <span class="simplepartwithsenses slidecontainer">
        <span class="headlabel slide">@Model[i].Title</span>
        <span class="selecttext slide">
            @Html.Partial("Ajax/MoreChangeDetail", new ModelDetails(@Model[i].Matches, @Model[i].Selection, @Model[i].Part))
        </span>
        <span class="simplebutton slide">
            @Html.Partial("Ajax/MoreSense", new ModelDetails("Definitions (" + @Model[i].Count + ")", @Model[i].Selection, @Model[i].Part))
        </span>
    </span>
}